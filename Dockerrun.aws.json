{
  "AWSEBDockerrunVersion": 2,
  "authentication": {
    "bucket": "liferay-docker-credentials-pulpo-engine-contacts",
    "key": "dockercfg"
  },
  "containerDefinitions": [
    {
      "environment": [
        {
          "name": "PULPO_CONTACT_ENGINE_ENVIRONMENT_UNIQUENAME",
          "value": "PULPO_CONTACT_ENGINE_ENVIRONMENT_UNIQUENAME"
        },
        {
          "name": "SPRING_PROFILES_ACTIVE",
          "value": "SPRING_PROFILES_ACTIVE"
        }
      ],
      "essential": true,
      "image": "liferay/com-liferay-osb-pulpo-engine-contacts-private:20170830T113546Z",
      "memory": 512,
      "name": "pulpo-engine-contacts",
      "portMappings": [
        {
          "containerPort": 8084,
          "hostPort": 80
        }
      ]
    },
    {
      "command": [
      "./filebeat",
      "-c",
      "/usr/share/filebeat/conf/filebeat.yml",
      "-e"
      ],
      "essential": true,
      "image": "docker.elastic.co/beats/filebeat:5.5.2",
      "memory": 256,
      "mountPoints": [
        {
          "containerPath": "/usr/share/filebeat/conf",
          "readOnly": true,
          "sourceVolume": "filebeatconfdir"
        },
        {
          "containerPath": "/var/log/aws",
          "readOnly": true,
          "sourceVolume": "awslogsdir"
        },
        {
          "containerPath": "/etc/pki/tls/certs",
          "readOnly": true,
          "sourceVolume": "certsdir"
        }
      ],
      "name": "pulpo-filebeat"
    }
  ],
  "volumes": [
    {
      "host": {
        "sourcePath": "/var/log/"
      },
      "name": "awslogsdir"
    },
    {
      "host": {
        "sourcePath": "/var/app/current/filebeat/conf"
      },
      "name": "filebeatconfdir"
    },
    {
      "host": {
        "sourcePath": "/var/app/current/logstash/certs"
      },
      "name": "certsdir"
    }
  ]
}
